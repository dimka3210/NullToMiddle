<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('/bundles/main/css/bootstrap.min.css') }}">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    {% block javascripts_start %}
        <script type="text/javascript" src="{{ asset('/bundles/main/js/jquery-2.2.2.min.js') }}"></script>
    {% endblock %}
</head>
<body>
{% include 'MainBundle::navbar.html.twig' %}
<div class="container">
    {% block content %}{% endblock %}
</div>
{% block javascripts %}
    <script type="text/javascript" src="{{ asset('/bundles/main/js/bootstrap.min.js') }}"></script>
{% endblock %}
</body>
</html>